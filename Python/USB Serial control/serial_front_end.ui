    <property name="styleSheet">
     <string notr="true">QCheckBox {
    color: #f5e9e2;  /* Text color (matches your QLabel) */
    font-size: 14px;
    spacing: 6px;    /* Space between checkbox and text */
}

/* Default checkbox (unchecked) */
QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #a58a7a;  /* Softer border (less harsh than #d9cdc6) */
    border-radius: 3px;
    background: #3a2e2a;  /* Darker background for better contrast */
}

/* Checked state (more elegant) */
QCheckBox::indicator:checked {
    background-color: #d4a373;  /* Warm highlight color (like QLabel.highlight) */
    border: 1px solid #d4a373;
    
}



/* Pressed state (click feedback) */
QCheckBox::indicator:pressed {
    background: #5c4a44;
}

/* Disabled state */
QCheckBox::indicator:disabled {
    border: 1px solid #6b8e7e;
    background: #3a3a3a;
}

/* Text color when checked (optional) */
QCheckBox:checked {
    color: #f5e9e2;  /* Keep original text color or use #d4a373 */
}</string>
